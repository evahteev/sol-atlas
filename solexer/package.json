{
  "name": "sqd-indexer",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config lib/main.js",
    "start:ts": "ts-node -r dotenv/config src/main.ts",
    "start:solana": "node lib/main-solana.js",
    "start:solana:ts": "ts-node src/main-solana.ts",
    "start:solana-rpc": "node lib/main-solana-rpc.js",
    "dev": "npm run build && npm run start",
    "dev:ts": "npm run start:ts",
    "dev:solana": "npm run build && npm run start:solana",
    "dev:solana:ts": "npm run start:solana:ts",
    "clean": "rm -rf lib",
    "rebuild": "npm run clean && npm run build",
    "process": "npm run dev",
    "process:ts": "npm run dev:ts",
    "migrate": "ts-node -r dotenv/config src/clickhouseClient/runMigrations.ts",
    "migrate:build": "npm run build && node -r dotenv/config lib/clickhouseClient/runMigrations.js",
    "docker:build": "docker build -t squid-indexer .",
    "docker:run": "docker run --env-file .env squid-indexer",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f indexer",
    "test:main-logic": "ts-node -r dotenv/config test-main-logic.ts"
  },
  "dependencies": {
    "@clickhouse/client": "^1.11.2",
    "@coral-xyz/borsh": "^0.32.1",
    "@jup-ag/instruction-parser": "^6.0.7",
    "@solana/web3.js": "^1.98.4",
    "@sqd-pipes/pipes": "^0.0.1-alpha.27",
    "@subsquid/batch-processor": "^0.0.0",
    "@subsquid/borsh": "0.2.0",
    "@subsquid/evm-abi": "^0.3.1",
    "@subsquid/evm-processor": "^1.27.1",
    "@subsquid/graphql-server": "^4.10.0",
    "@subsquid/solana-objects": "0.0.2",
    "@subsquid/solana-stream": "0.1.4",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@subsquid/typeorm-store": "^1.5.1",
    "@types/amqplib": "^0.10.7",
    "amqplib": "^0.10.8",
    "bs58": "^6.0.0",
    "dotenv": "^16.5.0",
    "typeorm": "^0.3.24"
  },
  "devDependencies": {
    "@subsquid/evm-typegen": "^4.4.0",
    "@subsquid/typeorm-codegen": "^2.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "private": true
}
